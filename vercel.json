{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/external-api/:path*",
      "destination": "https://musicverse-node-api.onrender.com/api/:path*"
    }
  ]
} 
